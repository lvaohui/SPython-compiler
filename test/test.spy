def<int> sum(int n):
    def<int> add(int a,int b):
        return a+b
    int i,s
    i = 0
    s = 0
    while i<=n:
        s = add(s,i)
        i = add(i,1)
    return s

def<int> factorial(int n):
    if n==0:
        return 1
    return n*factorial(n-1)
